/* jujiwuliu/pages/common/user/area_manager/apply.wxss */
page {
  background: #f4f4f4;
}

.form {
  width: 100%;
}

.input-box {
  width: calc(100% - 60rpx);
  height: 100%;
  display: flex;
  background-color: #fff;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.fui-cell {
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: solid 1px #f4f4f4;
  width: 100%;
  height: 90rpx;
}
.fui-cell::after {
  border-bottom: none;
}
.fui-cell .fui-cell-label {
  width: max-content;
  font-size: 28rpx;
}

.fui-cell .input {
  font-size: 28rpx;
  margin-left: 28rpx;
  flex: 1;
}

.fui-cell.lagre {
  height: 200rpx;
}

.fui-cell.lagre .input {
  flex: 1;
  font-size: 0;
}

.fui-cell.lagre .images {
  position: relative;
  width: 110rpx;
  height: 110rpx;
}

.fui-cell.lagre .images image {
  width: 110rpx;
  height: 110rpx;
  overflow: hidden;
  display: inline-block;
}